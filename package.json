{
  "name": "console.badge",
  "version": "0.0.0",
  "description": "Print badges inside browser console",
  "module": "index.js",
  "main": "dist/console.badge.cjs.js",
  "unpkg": "dist/console.badge.iife.js",
  "jsdelivr": "dist/console.badge.iife.js",
  "scripts": {
    "lint": "eslint .",
    "build": "bili",
